{
  "name": "lohost",
  "version": "0.0.1",
  "description": "Local virtual host router for development",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "lohost": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "optionalDependencies": {
    "@lohost/darwin-arm64": "0.0.1",
    "@lohost/darwin-x64": "0.0.1",
    "@lohost/linux-x64": "0.0.1",
    "@lohost/linux-arm64": "0.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "proxy",
    "localhost",
    "virtual-host",
    "development",
    "routing",
    "dns"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/websim-ai/lohost.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
